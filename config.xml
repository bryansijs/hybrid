<?xml version="1.0" encoding="utf-8"?>
<widget id="nl.downsider.hybridmaterial" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Pok√©dex</name>
    <description>
        Eindopdracht MBD1
    </description>
    <author email="" href="">
        Bryan Sijs Guus Iwanow
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" version="1" />
    <plugin name="cordova-plugin-geolocation"/>
    <plugin name="com.phonegap.plugins.OrientationLock" src="plugins/com.phonegap.plugins.OrientationLock"/>
    <plugin name="cordova-plugin-camera"/>
    <plugin name="cordova-plugin-device-motion"/>
    <plugin name="cordova-plugin-globalization"/>
    <plugin name="cordova-plugin-statusbar"/>
